#dashboard-wrap
  height 100%
  ul
    &.main-ul
      margin 0
      list-style-type none
      padding 0
      a
        font-size 15px
        color inherit
        text-decoration none
        display block
        margin-left 20px
        font-family "Roboto"
      li
        cursor pointer
        padding 7px
        &:hover
          background-color #4D4D4D
          -webkit-box-shadow inset -1px 0px 5px 0px rgba(0,0,0,0.75)
          -moz-box-shadow inset -1px 0px 5px 0px rgba(0,0,0,0.75)
          box-shadow inset -1px 0px 5px 0px rgba(0,0,0,0.75)
          color #c0c0c0
  nav
    background-color #FFAD00
    height 100%
    color white
    font-weight 100
    position fixed
    width 275px
    display none

#dashboard-wrap ul.main-ul a:visited,
#dashboard-wrap ul.main-ul a:active
  color inherit
  text-decoration none

.main-header
  color #ffffff
  padding 20px
  background #f4726d
  position fixed
  z-index 999
  width 100%
  top 0
  left 0
  right 0

#dashboard-content
  font-size 39px
  font-family "Open Sans"
  height 100%
  width 100%
  top 60px
  background-color rgb(246,246,228)

.mm-item

#logo-wrap
  width 100%
  height 160px
  padding 49px
  box-sizing border-box
  a
    font-size 45px

.nav-group
  padding-left 20px

.modal-header
  h5
    float left
  button
    float right

